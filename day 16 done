#include <stdio.h>

int main() {
    // For loop
    printf("For Loop:\n");
    for (int i = 1; i <= 5; i++) {
        printf("Iteration %d\n", i);
    }

    // While loop
    printf("\nWhile Loop:\n");
    int j = 1;
    while (j <= 5) {
        printf("Iteration %d\n", j);
        j++;
    }

    // Do-while loop
    printf("\nDo-While Loop:\n");
    int k = 1;
    do {
        printf("Iteration %d\n", k);
        k++;
    } while (k <= 5);

    return 0;
}
